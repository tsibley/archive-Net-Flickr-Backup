        <rdfs:seeAlso rdf:resource="http://www.flickr.com/photos/35034348999@N01/522214395#exif"/>
      </dcterms:StillImage>

      <flickr:tag rdf:about="http://www.flickr.com/photos/tags/cameraphone">
        <skos:prefLabel>cameraphone</skos:prefLabel>
      </flickr:tag>

